<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="HarmonyRememberAccountDialog" parent="AdwDialog">
        <style> <class name="remember_account_dialog"/> </style>
        <property name="title">Remember account?</property>
        <property name="content-width">400</property>
        <child>
            <object class="GtkBox">
                <property name="orientation">vertical</property>
                <child>
                    <object class="GtkLabel">
                        <style> <class name="header"/> </style>
                        <property name="label">Remember account?</property>
                    </object>
                </child>
                <child>
                    <object class="AdwAvatar" id="avatar">
                        <property name="size">200</property>
                        <property name="show_initials">true</property>
                        <child>
                            <object class="AdwSpinner" id="avatar_spinner">
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="global_name_label">
                        <property name="label"> </property>
                        <style> <class name="global_name_label"/> </style>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="username_label">
                        <property name="label"> </property>
                        <style> <class name="username_label"/> </style>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
